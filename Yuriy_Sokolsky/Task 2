function hashIntoArray(hash) {
  var array = Object.keys(hash);
  return array.map(function (key) {
    return [key, hash[key]];
  });
}

;
console.log(hashIntoArray({
  name: 'Jeremy',
  age: 24,
  role: 'Software Engineer'
}));
/*result::
[["name", "Jeremy"], ["age", 24], ["role", "Software Engineer"]]
*/
